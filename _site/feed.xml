                &lt;/span&gt;&lt;span class=&quot;n&quot;&gt;tableName&lt;/span&gt;&lt;span class=&quot;o&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;s&quot;&gt;&quot;JHI_AUTHORITY&quot;&lt;/span&gt;&lt;span class=&quot;x&quot;&gt;
